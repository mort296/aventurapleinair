<%= semantic_form_for [:admin, @useful_info], :html => { multipart: true }  do |f| %>
  <div class="language-tabs">
    <div id="lang-fr">
      <%= f.inputs 'Informations utile' do %>
        <%= f.input :name %>
        <%= f.input :icon, :as => :file %>
      <% end %>
    </div>
  </div>
  <%= f.actions %>
<% end %>

<script>
  
  $('.language-tabs').tabs();

</script>